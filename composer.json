{
    "name": "pronto/mobilebundle",
    "description": "A CMS bundle for mobile solutions",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Thomas Roovers",
            "email": "troovers@e-sites.nl"
        }
    ],
    "keywords": [
        "cms mobile pronto apps app bundle"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "troovers/browser": "^1.0.0",
        "ramsey/uuid": "^3.8",
        "doctrine/orm": "^2.5",
        "awps/font-awesome-php": "^4.7",
        "guzzlehttp/guzzle": "^6.3",
        "kreait/firebase-php": "^4.13",
        "league/flysystem": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "php-http/guzzle6-adapter": "^2.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "^4.4",
        "symfony/orm-pack": "^1.0",
        "symfony/form": "^4.4",
        "symfony/twig-bundle": "^4.4",
        "symfony/templating": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/serializer": "^4.4",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "symfony/proxy-manager-bridge": "^4.4",
        "symfony/expression-language": "^4.4",
        "composer/semver": "^1.4",
        "symfony/dotenv": "^4.4",
        "symfony/mime": "^4.4",
        "myclabs/php-enum": "^1.7",
        "trikoder/oauth2-bundle": "^3.2",
        "nyholm/psr7": "^1.3",
        "symfony/webpack-encore-bundle": "^1.12"
    },
    "require-dev": {
        "nelmio/alice": "^3.5.0",
        "symfony/phpunit-bridge": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Pronto\\MobileBundle\\": "src/",
            "Pronto\\MobileBundle\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
