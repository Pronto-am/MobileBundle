{
    "name": "pronto/mobilebundle",
    "description": "A CMS bundle for mobile solutions",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Thomas Roovers",
            "email": "troovers@e-sites.nl"
        }
    ],
    "keywords": [
        "cms mobile pronto apps app bundle"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "troovers/browser": "^1.1",
        "ramsey/uuid": "^3.9",
        "doctrine/orm": "^2.17",
        "awps/font-awesome-php": "^4.7",
        "guzzlehttp/guzzle": "^7.8",
        "kreait/firebase-php": "^7.0",
        "league/flysystem": "^1.0",
        "symfony/translation": "^6.4",
        "symfony/orm-pack": "^1.0",
        "symfony/form": "^6.4",
        "symfony/twig-bundle": "^6.4",
        "symfony/templating": "^6.4",
        "symfony/validator": "^6.4",
        "symfony/serializer": "^6.4",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "symfony/proxy-manager-bridge": "^6.4",
        "symfony/expression-language": "^6.4",
        "composer/semver": "^3.4",
        "symfony/dotenv": "^6.4",
        "symfony/mime": "^6.4",
        "myclabs/php-enum": "^1.7",
        "nyholm/psr7": "^1.3",
        "symfony/security-bundle": "^6.4",
        "league/oauth2-server-bundle": "^0.5",
        "symfony/routing": "^6.4",
        "symfony/mailer": "^6.4",
        "symfony/cache": "^6.4",
        "google/auth": "^1.33"
    },
    "require-dev": {
        "nelmio/alice": "^3.5.0",
        "symfony/phpunit-bridge": "^6.4",
        "rector/rector": "^0.18.11"
    },
    "autoload": {
        "psr-4": {
            "Pronto\\MobileBundle\\": "src/",
            "Pronto\\MobileBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.1"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
